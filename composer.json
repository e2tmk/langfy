{
    "name": "e2tmk/langfy",
    "description": "This is my package langfy",
    "keywords": [
        "langfy",
        "laravel",
        "langfy"
    ],
    "homepage": "https://github.com/langfy/langfy",
    "license": "MIT",
    "authors": [
        {
            "name": "Andr√© Domingues",
            "email": "dominguesstroppa@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Adryel Dearo",
            "email": "dearo.adryel@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "rector/rector": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Langfy\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Langfy\\LangfyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
